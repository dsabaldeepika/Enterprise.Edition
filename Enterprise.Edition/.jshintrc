{
  "browser": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "expr": false,
  "forin": true,
  "globals": {
    "jQuery": true,
    "angular": true,
    "toastr": true,
    "breeze": true,
    "moment": true,
    "q": true
  },
  "immed": true,
  "indent": 4,
  "latedef": "nofunc",
  "maxdepth": 4,
  "maxlen": 120,
  "maxparams": 10,
  "newcap": true,
  "noarg": true,
  "node": true,
  "nonew": true,
  "plusplus": false,
  "quotmark": "single",
  "strict": false,
  "trailing": true,
  "undef": false,
  "unused": false,
  "validthis": true
}
